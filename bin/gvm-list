#!/bin/bash
if [[ $1 == "list" ]]; then
  echo "
gvm gos
"
  if [ -f ~/.gvm/gos/default ]; then
    CUR_VER=`readlink $GVM_ROOT/gos/default | awk '{ n=split($1,path,"/"); print path[n] }'`
  else
    CUR_VER=asdfqwerzxcv
  fi
  ls -1 $GVM_ROOT/gos | grep ^go | sed 's/^/   /g' $SELECTOR
  echo
else
  echo "Please use gvm [action] to call this file"
  exit 1
fi

