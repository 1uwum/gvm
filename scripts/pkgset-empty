#!/bin/bash
if [[ $gvm_go_name == "" ]]; then
  echo "ERROR: No Go version selected"
  exit 1
elif [[ $gvm_pkgset_name == "" ]]; then
  echo "ERROR: No Package set selected"
  exit 1
fi

rm -rf $GVM_ROOT/pkgsets/$gvm_go_name/$gvm_pkgset_name/*
if [[ $? -ne 0 ]]; then
  echo "ERROR: Could not delete package set files"
  exit 1
fi

